                                    local cf = game.Workspace.CurrentCamera.CFrame
                                    twister:SetAttribute("Paused",true)
                                    contact = game:GetService("RunService").RenderStepped:Connect(function()
                                        game.Workspace.CurrentCamera.CFrame = cf
                                    end)
